//Using Third Variable


#include<stdio.h>

int main()
{

    int a,b,c;
    printf("Enter Two Numbers\n");
    scanf("%d%d",&a,&b);
    printf("Value of a :%d,\nValue of b : %d\n",a,b);
    c=a;
    a=b;
    b=c;
    printf("Value of a :%d\n",a);
    printf("Value of b :%d\n",b);
    return 0;
}


//Not using Third Variable

#include<stdio.h>

int main()
{

    int a,b;
    printf("Enter Two Numbers\n");
    scanf("%d%d",&a,&b);
    printf("Value of a :%d\nValue of b : %d\n",a,b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("Value of a :%d\n",a);
    printf("Value of b :%d\n",b);
    return 0;


}
